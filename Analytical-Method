        "# Import the necessary modules\n",
        "import os\n",
        "\n",
        "arquivo= 'PM10_04-05-2022_IN_PRI.20220729084003.txt'\n",
        "\n",
        "# Check if the file exists\n",
        "if not os.path.isfile(arquivo):\n",
        "    print(\"File does not exist\")\n",
        "    exit()\n",
        "# Open the file and read the data\n",
        "with open(arquivo, 'r') as file:\n",
        "    data = file.read()\n",
        "\n",
        "# Split the data into lines\n",
        "lines = data.split('\\n')\n",
        "\n",
        "# Filter the lines that contain 'Quan-FP'\n",
        "filtered_lines = [line.strip(\" \\t\")  for line in lines if 'Quan-FP' in line]\n",
        "\n",
        "for line in filtered_lines:\n",
        "  #print(line)\n",
        "  dados = line.split()\n",
        "  if len(dados[6]) == 1:\n",
        "      dados[8] = float(dados[8].replace(\", \", ''))\n",
        "  else:\n",
        "      dados[7] = float(dados[7].replace(',', ''))\n",
        "  if len(dados[6]) == 1:\n",
        "        print(dados[0], dados[8])\n",
        "  else:\n",
        "      print(dados[0], dados[7])\n",
        "\n"
      ],
